        <th>流入</th>\n              <th>流出</th>\n              <th>金额</th>\n              <th>类别</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"item in items\">\n              <td>item.roll_in</td>\n              <td>item.roll_out</td>\n              <td>item.amount</td>\n              <td v-if=\"item.type == 0\">交易</td>\n              <td v-if=\"item.type == 1\">佣金</td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import axios from \"axios\";\n\n  export default {\n    data() {\n      return {\n        select: 0,\n        account_id: 0,\n        items: [],\n      };\n    },\n    mounted() {\n      this.account_id = this.$route.params.id;\n    },\n    methods: {\n      getInfo() {\n        var formData = {\n          authorize: JSON.parse(localStorage.getItem(\"authorize\") || \"{}\"),\n          select: this.select,\n        };\n        axios.post(\"/api/flow\", formData).then((response) => {\n          this.items = response.items;\n        });\n      },\n      go_back() {\n        this.$router.back();\n      },\n    },\n    watch: {\n      select: {\n        handler() {\n          this.getInfo();\n        },\n      },\n    },\n  };\n</script>\n"],"sourceRoot":""}]}