his.items = response.items;\n        });\n      },\n      go_back() {\n        this.$router.back();\n      },\n    },\n    watch: {\n      select: {\n        handler() {\n          this.getInfo();\n        },\n      },\n    },\n  };\n</script>\n"],"sourceRoot":""}]}