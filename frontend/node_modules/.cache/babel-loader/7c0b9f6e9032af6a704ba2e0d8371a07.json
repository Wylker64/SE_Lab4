parse(localStorage.getItem('authorize') || '{}').username == 'admin'\n    }\n    axios\n        .post('/shop', formData)\n        .then(response => {\n              this.shops = JSON.parse(JSON.stringify((response.data.shops)))\n              console.log(this.shops)\n        })\n      .catch(error => {\n        console.log(error)\n      })\n\n    this.$store.commit('setIsLoading', false)\n    },\n\n\n  }\n}\n</script>\n"],"sourceRoot":""}]}