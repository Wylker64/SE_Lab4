R,CAAY,IAAI,CAAC,SAAL,CAAe,KAAf,CAAZ;AACJ;AACH,SArCL;AAsCJ;AACH,KA1KI;AA2KL,IAAA,OA3KK,qBA2KK;AAAA;;AACN,WAAK,MAAL,GAAc,EAAd;;AACA,UAAI,CAAC,KAAK,MAAL,CAAY,MAAjB,EAAyB;AACrB,YAAM,QAAO,GAAI;AACb,UAAA,OAAO,EAAE,KAAK;AADD,SAAjB;AAIA,QAAA,KAAI,CACC,IADL,CACU,GADV,EACe,QADf,EAEK,IAFL,CAEU,UAAA,QAAO,EAAK;AACd,cAAI,QAAQ,CAAC,IAAT,CAAc,OAAd,IAAyB,IAA7B,EAAmC;AAC/B,YAAA,KAAK,CAAC;AACF,cAAA,OAAO,EAAE,kBADP;AAEF,cAAA,IAAI,EAAE,YAFJ;AAGF,cAAA,WAAW,EAAE,IAHX;AAIF,cAAA,YAAY,EAAE,IAJZ;AAKF,cAAA,QAAQ,EAAE,IALR;AAMF,cAAA,QAAQ,EAAE;AANR,aAAD,CAAL;AASJ,WAVA,MAUO;AACH,YAAA,KAAK,CAAC;AACF,cAAA,OAAO,EAAE,QAAQ,CAAC,IAAT,CAAc,OADrB;AAEF,cAAA,IAAI,EAAE,SAFJ;AAGF,cAAA,WAAW,EAAE,IAHX;AAIF,cAAA,YAAY,EAAE,IAJZ;AAKF,cAAA,QAAQ,EAAE,IALR;AAMF,cAAA,QAAQ,EAAE;AANR,aAAD,CAAL;AASJ;AACH,SAxBL,WAyBW,UAAA,KAAI,EAAK;AACZ,cAAI,KAAK,CAAC,QAAV,EAAoB;AAChB,iBAAK,IAAM,QAAX,IAAuB,KAAK,CAAC,QAAN,CAAe,IAAtC,EAA4C;AACxC,cAAA,MAAI,CAAC,MAAL,CAAY,IAAZ,WAAoB,QAApB,eAAiC,KAAK,CAAC,QAAN,CAAe,IAAf,CAAoB,QAApB,CAAjC;AACJ;;AAEA,YAAA,OAAO,CAAC,GAAR,CAAY,IAAI,CAAC,SAAL,CAAe,KAAK,CAAC,QAAN,CAAe,IAA9B,CAAZ;AACJ,WANA,MAMO,IAAI,KAAK,CAAC,OAAV,EAAmB;AACtB,YAAA,MAAI,CAAC,MAAL,CAAY,IAAZ,CAAiB,wCAAjB;;AAEA,YAAA,OAAO,CAAC,GAAR,CAAY,IAAI,CAAC,SAAL,CAAe,KAAf,CAAZ;AACJ;AACH,SArCL;AAsCJ;AACJ;AAzNK;AAfE,CAAf","sourcesContent":["<template>\n    <div class=\"page-seller\">\n        <div class=\"columns\">\n            <div class=\"column is-7\">\n                <h1 class=\"title\">上架申请记录</h1>\n\n                <form @submit.prevent=\"getOrder\">\n                    <div class=\"column is-2\">\n                        <div class=\"field\">\n                            <label>Page</label>\n                            <div class=\"control\">\n                                <input type=\"text\" class=\"input\" v-model=\"pageid\">\n                            </div>\n                        </div>\n                        <div class=\"field\">\n                            <div class=\"control\">\n                                <button class=\"button is-dark\">搜索</button>\n                            </div>\n                        </div>\n                    </div>\n                </form>\n                <table class=\"table is-fullwidth\">\n                    <thead>\n                        <tr>\n                            <th>商品名称</th>\n                            <th>申请状态</th>\n                        </tr>\n                    </thead>\n\n                    <tbody>\n                        <tr v-for=\"item in items\" v-bind:key=\"item.order_id\">\n                            <td>{{ item.name }}</td>\n                            <td>\n                                <button v-if=\"item.status === '1'\" @click=\"actionForStatus1(item.prod_id)\">申请成功</button>\n                                <button v-else-if=\"item.status === '2'\"\n                                    @click=\"actionForStatus2(item.prod_id)\">申请失败</button>\n                                <button v-else-if=\"item.status === '3'\"\n                                    @click=\"actionForStatus3(item.prod_id)\">正在申请</button>\n                            </td>\n\n                        </tr>\n                    </tbody>\n                </table>\n\n                <br>\n                <div class=\"columns\">\n\n                    <div class=\"column\">\n                        <h1 class=\"title\">添加商品</h1>\n\n                        <form @submit.prevent=\"add\">\n\n                            <div class=\"field\">\n                                <label>商品名</label>\n                                <div class=\"control\">\n                                    <input type=\"text\" class=\"input\" v-model=\"name\">\n                                </div>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>商品描述</label>\n                                <div class=\"control\">\n                                    <input type=\"text\" class=\"input\" v-model=\"description\">\n                                </div>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>价格</label>\n                                <div class=\"control\">\n                                    <input type=\"text\" class=\"input\" v-model=\"price\">\n                                </div>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>添加图片</label>\n                                <div class=\"control\">\n                                    <input type=\"file\" class=\"input\" @change=\"onFileChange\">\n                                </div>\n                            </div>\n\n\n\n                            <div class=\"notification is-danger\" v-if=\"errors.length\">\n                                <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n                            </div>\n                            <br />\n                            <div class=\"field\">\n                                <div class=\"control\">\n                                    <button class=\"button is-dark\" @click=\"add()\">添加商品</button>\n                                </div>\n                            </div>\n                        </form>\n                    </div>\n                    <hr>\n                    <div class=\"column\">\n                        <h1 class=\"title\">更新商品</h1>\n\n                        <form @submit.prevent=\"add\">\n                            <div class=\"field\">\n                                <label>商品编号</label>\n                                <div class=\"control\">\n                                    <input type=\"text\" class=\"input\" v-model=\"name\">\n                                </div>\n                            </div>\n                            <div class=\"field\">\n                                <label>商品名</label>\n                                <div class=\"control\">\n                                    <input type=\"text\" class=\"input\" v-model=\"name\">\n                                </div>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>商品描述</label>\n                                <div class=\"control\">\n                                    <input type=\"text\" class=\"input\" v-model=\"description\">\n                                </div>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>价格</label>\n                                <div class=\"control\">\n                                    <input type=\"text\" class=\"input\" v-model=\"price\">\n                                </div>\n                            </div>\n\n                            <div class=\"field\">\n                                <label>添加图片</label>\n                                <div class=\"control\">\n                                    <input type=\"file\" class=\"input\" @change=\"onFileChange\">\n                                </div>\n                            </div>\n\n\n\n                            <div class=\"notification is-danger\" v-if=\"errors.length\">\n                                <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n                            </div>\n                            <br />\n                            <div class=\"field\">\n                                <div class=\"control\">\n                                    <button class=\"button is-dark\" @click=\"update()\">更新商品</button>\n                                </div>\n                            </div>\n                        </form>\n                    </div>\n                </div>\n\n                <div class=\"column is-4\">\n                    <h1 class=\"title\">删除商品</h1>\n                    <form @submit.prevent=\"deleteP\">\n                        <div class=\"field\">\n                            <label>商品id</label>\n                            <div class=\"control\">\n                                <input type=\"text\" class=\"input\" v-model=\"prod_id\">\n                            </div>\n                        </div>\n\n                        <div class=\"notification is-danger\" v-if=\"errors.length\">\n                            <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n                        </div>\n\n                        <div class=\"field\">\n                            <div class=\"control\">\n                                <button class=\"button is-dark\">删除商品</button>\n                            </div>\n                        </div>\n                    </form>\n                </div>\n            </div>\n            <div class=\"column is-3 is-offset-2\">\n                <button class=\"button is-danger\">删除商店</button>\n                <div style=\"height: 5rem;\"></div>\n                <h1 class=\"title\">存款</h1>\n                <form @submit.prevent=\"\">\n                    <div class=\"field\">\n                        <label>存储金额</label>\n                        <div class=\"control\">\n                            <input type=\"text\" class=\"input\" v-model=\"prod_id\">\n                        </div>\n                    </div>\n\n                    <div class=\"field\">\n                        <div class=\"control\">\n                            <button class=\"button is-dark\">存款</button>\n                        </div>\n                    </div>\n                </form>\n\n                <div style=\"height: 5rem;\"></div>\n                <h1 class=\"title\">转账</h1>\n                <form @submit.prevent=\"\">\n                    <div class=\"field\">\n                        <label>转账金额</label>\n                        <div class=\"control\">\n                            <input type=\"text\" class=\"input\" v-model=\"prod_id\">\n                        </div>\n                    </div>\n\n                    <div class=\"field\">\n                        <div class=\"control\">\n                            <button class=\"button is-dark\">转账</button>\n                        </div>\n                    </div>\n                </form>\n\n                <div style=\"height: 5rem;\"></div>\n                <h1 class=\"title\">信息修改</h1>\n                <form @submit.prevent=\"submitForm\">\n\n                    <div type=\"field\">\n                        <label>店名</label>\n                        <div type=\"control\">\n                            <input type=\"input\" v-model=\"shopname\">\n                        </div>\n                    </div>\n\n                    <div type=\"field\">\n                        <label>售卖商品类别</label>\n                        <div type=\"control\">\n                            <input type=\"input\" v-model=\"type\">\n                        </div>\n                    </div>\n\n                    <div type=\"field\">\n                        <label>商店简介</label>\n                        <div type=\"control\">\n                            <input type=\"input\" v-model=\"description\">\n                        </div>\n                    </div>\n\n                    <div type=\"field\">\n                        <label>备案地址</label>\n                        <div type=\"control\">\n                            <input type=\"input\" v-model=\"address\">\n                        </div>\n                    </div>\n\n                    <div type=\"field\">\n                        <label>注册资金</label>\n                        <div type=\"control\">\n                            <input type=\"input\" v-model=\"capital\">\n                        </div>\n                    </div>\n\n                    <div type=\"field\">\n                        <label>注册时间</label>\n                        <div type=\"control\">\n                            <input type=\"input\" v-model=\"date\">\n                        </div>\n                    </div>\n\n                    <div type=\"notification is-danger\" v-if=\"errors.length\">\n                        <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n                    </div>\n\n                    <div type=\"field\">\n                        <div type=\"control\">\n                            <button type=\"button is-dark\">修改！</button>\n                        </div>\n                    </div>\n\n                    <hr>\n\n\n                </form>\n            </div>\n\n        </div>\n\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport { toast } from 'bulma-toast'\nimport { toRaw } from '@vue/reactivity';\nexport default {\n    name: 'AddProduct',\n    data() {\n        return {\n            image: null,\n            name: '',\n            description: '',\n            price: '',\n            errors: [],\n            items: [],\n        }\n    },\n    mounted() {\n        document.title = ''\n    },\n    methods: {\n        onFileChange(event) {\n            const file = event.target.files[0];\n            if (file) {\n                this.image = file;\n            }\n        },\n        refund(id) {\n            this.errors = []\n            console.log(this.id)\n            if (!this.errors.length) {\n                var formData = {\n                    order_id: String(id),\n                    is_agreed: 'true'\n                }\n\n                axios\n                    .post('', formData)\n                    .then(response => {\n\n                        if (response.data.success == true) {\n\n                            toast({\n                                message: '!',\n                                type: 'is-success',\n                                dismissible: true,\n                                pauseOnHover: true,\n                                duration: 2000,\n    