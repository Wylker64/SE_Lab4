                <input type=\"text\" class=\"input\" v-model=\"price\">\n                        </div>\n                    </div>\n\n                    <div class=\"field\">\n                        <label>Quantity</label>\n                        <div class=\"control\">\n                            <input type=\"text\" class=\"input\" v-model=\"quantity\">\n                        </div>\n                    </div>\n\n                    <!-- <div id=\"file-js-example\" class=\"file has-name\">\n                    <label class=\"file-label\">\n                        <input class=\"file-input\" type=\"file\" name=\"resume\">\n                        <span class=\"file-cta\">\n                        <span class=\"file-icon\">\n                            <i class=\"fas fa-upload\"></i>\n                        </span>\n                        <span class=\"file-label\">\n                            Choose a file…\n                        </span>\n                        </span>\n                        <span class=\"file-name\">\n                        No file uploaded\n                        </span>\n                    </label>\n                    </div> -->\n\n\n                    <div class=\"notification is-danger\" v-if=\"errors.length\">\n                        <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n                    </div>\n                    <br /> \n                    <div class=\"field\">\n                        <div class=\"control\">\n                            <button class=\"button is-dark\" @click=\"add()\">Add Product</button>\n                        </div>\n                    </div>\n                </form>\n                <hr>\n            </div>\n            <div class=\"column is-4 is-offset-4\">\n                <h1 class=\"title\">Update Inventory</h1>\n\n                <form @submit.prevent=\"update\">\n                <div class=\"field\">\n                    <label>Product id</label>\n                    <div class=\"control\">\n                        <input type=\"text\" class=\"input\" v-model=\"prod_id\">\n                    </div>\n                </div>\n\n                <div class=\"field\">\n                    <label>Quantity</label>\n                    <div class=\"control\">\n                        <input type=\"text\" class=\"input\" v-model=\"quantity\">\n                    </div>\n                </div>\n\n                <div class=\"notification is-danger\" v-if=\"errors.length\">\n                    <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n                </div>\n\n                <div class=\"field\">\n                    <div class=\"control\">\n                        <button class=\"button is-dark\">Update Inventory</button>\n                    </div>\n                </div>\n                </form>\n                <hr>\n            </div>\n            <div class=\"column is-4 is-offset-4\">\n                <h1 class=\"title\">Delete Product</h1>\n                <form @submit.prevent=\"deleteP\">\n                <div class=\"field\">\n                    <label>Product id</label>\n                    <div class=\"control\">\n                        <input type=\"text\" class=\"input\" v-model=\"prod_id\">\n                    </div>\n                </div>\n\n                <div class=\"notification is-danger\" v-if=\"errors.length\">\n                    <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n                </div>\n\n                <div class=\"field\">\n                    <div class=\"control\">\n                        <button class=\"button is-dark\">Delete Product</button>\n                    </div>\n                </div>\n                </form>\n            </div>\n            </div>\n\n\n</template>\n\n<script>\nimport axios from 'axios'\nimport { toast } from 'bulma-toast'\nimport { toRaw } from '@vue/reactivity';\nexport default {\n    name: 'AddProduct',\n    data() {\n        return {\n            name: '',\n            description: '',\n            price: '',\n            quantity: '',\n            errors: [],\n            items:[],\n            dict:{0:'Normal', 1:'Asking', 2:'Agreed', 3:'Refused'}\n        }\n    },\n    mounted() {\n        document.title = 'Manage Inventory'\n    },\n    methods: {\n        refund(id) {\n            this.errors = []\n            console.log(this.id)\n            if (!this.errors.length) {\n                var formData =  {\n                    order_id: String(id),\n                    is_agreed: 'true'\n                }\n\n                axios\n                    .post('/seller/refund', formData)\n                    .then(response => {\n                        \n                        if (response.data.success == true){\n\n                            toast({\n                            message: 'Finished!',\n                            type: 'is-success',\n                            dismissible: true,\n                            pauseOnHover: true,\n                            duration: 2000,\n                            position: 'bottom-right',\n                            \n                        })\n                        } else{\n                            toast({\n                            message: response.data.message,\n                            type: 'warning',\n                            dismissible: true,\n                            pauseOnHover: true,\n                            duration: 2000,\n                            position: 'bottom-right',                            \n                        })\n\n                        }\n                    })\n                    .catch(error => {\n                        if (error.response) {\n                            for (const property in error.response.data) {\n                                this.errors.push(`${property}: ${error.response.data[property]}`)\n                            }\n\n                            console.log(JSON.stringify(error.response.data))\n                        } else if (error.message) {\n                            this.errors.push('Something went wrong. Please try again')\n                            \n                            console.log(JSON.stringify(error))\n                        }\n                    })\n            }\n        },\n        getOrder() {\n        var formData =  {\n            pageid: this.pageid,\n        }\n        axios\n            .post('/seller/order', formData)\n            .then(response => {\n                  this.items = response.data.order_list;\n                  this.items = toRaw(this.items)\n                  console.log(document.getElementById(\"agree\"))\n            })\n          .catch(error => {\n            console.log(error)\n          })\n  \n      },\n        add() {\n            this.errors = []\n            if (!this.errors.length) {\n                const formData =  {\n                    prodname: this.name,\n                    descri: this.description,\n                    price: this.price,\n                    quantity: this.quantity\n                }\n\n                axios\n                    .post('/seller/addproduct', formData)\n                    .then(response => {\n                        if (response.data.success == true){\n                            toast({\n                            message: 'Product Added!',\n                            type: 'is-success',\n                            dismissible: true,\n                            pauseOnHover: true,\n                            duration: 2000,\n                            position: 'bottom-right',\n                            \n                        })\n                        } else{\n                            toast({\n                            message: response.data.message,\n                            type: 'warning',\n                            dismissible: true,\n                            pauseOnHover: true,\n                            duration: 2000,\n                            position: 'bottom-right',                            \n                        })\n\n                        }\n                    })\n                    .catch(error => {\n                        if (error.response) {\n                            for (const property in error.response.data) {\n                                this.errors.push(`${property}: ${error.response.data[property]}`)\n                            }\n\n                            console.log(JSON.stringify(error.response.data))\n                        } else if (error.message) {\n                            this.errors.push('Something went wrong. Please try again')\n                            \n                            console.log(JSON.stringify(error))\n                        }\n                    })\n            }\n        },\n        update() {\n            this.errors = []\n            if (!this.errors.length) {\n                const formData =  {\n                    prod_id: this.prod_id,\n                    quantity: this.quantity\n                }\n\n                axios\n                    .post('/seller/modifyproduct', formData)\n                    .then(response => {\n                        if (response.data.success == true){\n                            toast({\n                            message: 'Inventory Updated!',\n                            type: 'is-success',\n                            dismissible: true,\n                            pauseOnHover: true,\n                            duration: 2000,\n                            position: 'bottom-right',\n                            \n                        })\n                        } else{\n                            toast({\n                            message: response.data.message,\n                            type: 'warning',\n                            dismissible: true,\n                            pauseOnHover: true,\n                            duration: 2000,\n                            position: 'bottom-right',                            \n                        })\n\n                        }\n                    })\n                    .catch(error => {\n                        if (error.response) {\n                            for (const property in error.response.data) {\n                                this.errors.push(`${property}: ${error.response.data[property]}`)\n                            }\n\n                            console.log(JSON.stringify(error.response.data))\n                        } else if (error.message) {\n                            this.errors.push('Something went wrong. Please try again')\n                            \n                            console.log(JSON.stringify(error))\n                        }\n                    })\n            }\n        },\n        deleteP() {\n            this.errors = []\n            if (!this.errors.length) {\n                const formData =  {\n                    prod_id: this.prod_id,\n                }\n\n                axios\n                    .post('/seller/deleteproduct', formData)\n                    .then(response => {\n                        if (response.data.success == true){\n                            toast({\n                            message: 'Product Deleted!',\n                            type: 'is-success',\n                            dismissible: true,\n                            pauseOnHover: true,\n                            duration: 2000,\n                            position: 'bottom-right',\n                            \n                        })\n                        } else{\n                            toast({\n                            message: response.data.message,\n                            type: 'warning',\n                            dismissible: true,\n                            pauseOnHover: true,\n                            duration: 2000,\n                            position: 'bottom-right',                            \n                        })\n\n                        }\n                    })\n                    .catch(error => {\n                        if (error.response) {\n                            for (const property in error.response.data) {\n                                this.errors.push(`${property}: ${error.response.data[property]}`)\n                            }\n\n                            console.log(JSON.stringify(error.response.data))\n                        } else if (error.message) {\n                            this.errors.push('Something went wrong. Please try again')\n                            \n                            console.log(JSON.stringify(error))\n                        }\n                    })\n            }\n        }\n    }\n}\n</script>"]}]}