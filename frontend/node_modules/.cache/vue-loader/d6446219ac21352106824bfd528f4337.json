        v-bind:key=\"item.order_id\"\n                >\n                    <td>{{ item.order_id}}</td>\n                    <td>${{ item.price }}</td>\n                    <td>{{ item.buy_num }}</td>\n                    <td>${{ item.total_price }}</td>\n                    <td>{{ dict[item.return_status] }}</td>\n                    <td><button v-if=\"item.return_status === '1'\" @click=\"refund(item.prod_id)\">Agree</button></td>\n                </tr>\n            </tbody>\n        </table>\n        </div>\n        <br>\n        <hr>\n        <div class=\"column is-4 is-offset-4\">\n                <h1 class=\"title\">Add Product</h1>\n\n                <form @submit.prevent=\"add\">\n\n                    <div class=\"field\">\n                        <label>Product Name</label>\n                        <div class=\"control\">\n                            <input type=\"text\" class=\"input\" v-model=\"name\">\n                        </div>\n                    </div>\n\n                    <div class=\"field\">\n                        <label>Description</label>\n                        <div class=\"control\">\n                            <input type=\"text\" class=\"input\" v-model=\"description\">\n                        </div>\n                    </div>\n\n                    <div class=\"field\">\n                        <label>Price</label>\n                        <div class=\"control\">\n                            <input type=\"text\" class=\"input\" v-model=\"price\">\n                        </div>\n                    </div>\n\n                    <div class=\"field\">\n                        <label>Quantity</label>\n                        <div class=\"control\">\n                            <input type=\"text\" class=\"input\" v-model=\"quantity\">\n                        </div>\n                    </div>\n\n                    <!-- <div id=\"file-js-example\" class=\"file has-name\">\n                    <label class=\"file-label\">\n                        <input class=\"file-input\" type=\"file\" name=\"resume\">\n                        <span class=\"file-cta\">\n                        <span class=\"file-icon\">\n                            <i class=\"fas fa-upload\"></i>\n                        </span>\n                        <span class=\"file-label\">\n                            Choose a file…\n                        </span>\n                        </span>\n                        <span class=\"file-name\">\n                        No file uploaded\n                        </span>\n                    </label>\n                    </div> -->\n\n\n                    <div class=\"notification is-danger\" v-if=\"errors.length\">\n                        <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n                    </div>\n                    <br /> \n                    <div class=\"field\">\n                        <div class=\"control\">\n                            <button class=\"button is-dark\" @click=\"add()\">Add Product</button>\n                        </div>\n                    </div>\n                </form>\n                <hr>\n            </div>\n            <div class=\"column is-4 is-offset-4\">\n                <h1 class=\"title\">Update Inventory</h1>\n\n                <form @submit.prevent=\"update\">\n                <div class=\"field\">\n                    <label>Product id</label>\n                    <div class=\"control\">\n                        <input type=\"text\" class=\"input\" v-model=\"prod_id\">\n                    </div>\n                </div>\n\n                <div class=\"field\">\n                    <label>Quantity</label>\n                    <div class=\"control\">\n                        <input type=\"text\" class=\"input\" v-model=\"quantity\">\n                    </div>\n                </div>\n\n                <div class=\"notification is-danger\" v-if=\"errors.length\">\n                    <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n                </div>\n\n                <div class=\"field\">\n                    <div class=\"control\">\n                        <button class=\"button is-dark\">Update Inventory</button>\n                    </div>\n                </div>\n                </form>\n                <hr>\n            </div>\n            <div class=\"column is-4 is-offset-4\">\n                <h1 class=\"title\">Delete Product</h1>\n                <form @submit.prevent=\"deleteP\">\n                <div class=\"field\">\n                    <label>Product id</label>\n                    <div class=\"control\">\n                        <input type=\"text\" class=\"input\" v-model=\"prod_id\">\n                    </div>\n                </div>\n\n                <div class=\"notification is-danger\" v-if=\"errors.length\">\n                    <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n                </div>\n\n                <div class=\"field\">\n                    <div class=\"control\">\n                        <button class=\"button is-dark\">Delete Product</button>\n                    </div>\n                </div>\n                </form>\n            </div>\n            </div>\n\n\n</template>\n\n<script>\nimport axios from 'axios'\nimport { toast } from 'bulma-toast'\nimport { toRaw } from '@vue/reactivity';\nexport default {\n    name: 'AddProduct',\n    data() {\n        return {\n            name: '',\n            description: '',\n            price: '',\n            quantity: '',\n            errors: [],\n            items:[],\n            dict:{0:'Normal', 1:'Asking', 2:'Agreed', 3:'Refused'}\n        }\n    },\n    mounted() {\n        document.title = 'Manage Inventory'\n    },\n    methods: {\n        refund(id) {\n            this.errors = []\n            console.log(this.id)\n            if (!this.errors.length) {\n                var formData =  {\n                    order_id: String(id